    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.8.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.3",
