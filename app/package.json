{
  "name": "team7chat",
  "version": "1.0.0",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "test": "npm run lint",
    "lint": "eslint . --ext .js,.jsx",
    "deploy": "now --public -e NODE_ENV=production --token $NOW_TOKEN --npm",
    "alias": "now alias --token=$NOW_TOKEN"
  },
  "dependencies": {
    "express": "^4.14.0",
    "next": "^5.0.0",
    "prop-types": "^15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0"
  },
  "devDependencies": {
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "5.0.1",
    "now": "^10.2.1",
    "supertest": "3.0.0"
  },
  "now": {
    "name": "team7chat",
    "alias": "team7chat"
  }
}
